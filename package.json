{
  "name": "algosdk-node6",
  "version": "1.0.0",
  "description": "This is a soft fork of algosdk targetting node 6 builds",
  "main": "index.js",
  "scripts": {
    "build": "babel ./node_modules/algosdk/src --out-dir build/",
    "buildBabel": "browserify build/main.js -s algosdk | minify --simplify --mangle.keepClassName --deadcode.keepFnArgs --mangle.keepFnName --comments false --outFile dist/algosdk.babel.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "post_install": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "hi-base32": "^0.5.0",
    "js-sha512": "^0.8.0",
    "msgpack-lite": "git+https://github.com/algorand/msgpack-lite.git",
    "algosdk": "^1.1.1",
    "tweetnacl": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/preset-env": "^7.4.2",
    "babel-minify": "^0.5.0",
    "browserify": "^16.2.3"
  }
}
